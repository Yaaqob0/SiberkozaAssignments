    "import pandas as pd\n",
    "from sklearn.tree import DecisionTreeClassifier\n",
    "movie_data = pd.read_csv('movie.csv')\n",
    "x = movie_data.drop(columns = ['Category'])\n",
    "y = movie_data['Category']\n",
    "\n",
    "model = DecisionTreeClassifier()\n",
    "model.fit(x, y)\n",
    "prediction = model.predict([[19,1],[33,0]])\n",
    "prediction"
   ]
  }
